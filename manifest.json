{
  "name": "Jisho",
  "description": "Jisho.org extension",
  "version": "0.1",
  "manifest_version": 2,
  "permissions": [
    "contextMenus"
  ],
  "host_permissions": [],
  "browser_action": {
    "default_title": "Jisho",
    "default_icon": "assets/icon-32.png",
    "default_popup": "popup/popup.html"
  },
  "icons": {
    "16": "assets/icon-16.png",
    "32": "assets/icon-32.png",
    "48": "assets/icon-48.png",
    "128": "assets/icon-128.png"
  },
  "key": ""
}

