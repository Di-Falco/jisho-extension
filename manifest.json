{
  "name": "Jisho",
  "description": "Japanese-English dictionary access, on any webpage",
  "version": "1.2.1",
  "manifest_version": 3,
  "action": {
    "default_title": "Jisho",
    "default_icon": "assets/icon.png",
    "default_popup": "popup/popup.html"
  },
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "service_worker": "popup.js",
    "type": "module"
  }
}